@GLOSSDEF{Perihelio,
	word = {Perihelio},
	definition = {Punto más cercano de la órbita de un cuerpo celeste alrededor del Sol}
}

@GLOSSDEF{Afelio,
	word = {Afelio},
	definition = {Punto más alejado de la órbita de un planeta alrededor del Sol}
}

@GLOSSDEF{gi,
        word = {GI},
        definition = {Radiación Global, Global Irradiation(del ingles)}
}
@GLOSSDEF{dni,
        word = {DNI},
        definition = {Radiación Normal Directa, Direct Normal Irradiation(del ingles). Es la radiación solar medida en dirección normal al sol}
}

@GLOSSDEF{rd,
        word = {RD},
	definition = {Radiación Difusa, Cuándo la radiación choca con moléculas en la atmósfera esta se dispersa, la radiación dispersada que alcanza la superficie terrestre se denomina Radiación difusa.}
}

@GLOSSDEF{GHI,
        word = {GHI},
        definition = {Radiación Global Horizontal, Global Horizontal Iradiation(del ingles), Radiación solar mediada en el plano horizontal, es decir en el plano o superficia donde se encuentran los instrumentos de medición.}
}

